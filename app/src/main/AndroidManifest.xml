<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.generict.shoppingwithfriends" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.generict.shoppingwithfriends.LoginRegistrationActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.generict.shoppingwithfriends.LoginPageActivity"
            android:label="@string/title_activity_login_page" >
        </activity>
        <activity
            android:name="com.generict.shoppingwithfriends.ApplicationHomeActivity"
            android:label="@string/title_activity_application_home" >
        </activity>
    </application>

</manifest>
